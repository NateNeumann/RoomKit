<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="edu.dartmouthcs65.museumtour.MainMapFragment">




    <ImageView
        android:id="@+id/rm_0_img"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/rm_0_selected_v1"
        android:scaleType="centerCrop"/>

    <ImageView
        android:id="@+id/rm_1_img"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/rm_1_selected_v1"
        android:scaleType="centerCrop"/>

    <ImageView
        android:id="@+id/rm_2_img"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/rm_2_selected_v1"
        android:scaleType="centerCrop"/>

    <ImageView
        android:id="@+id/rm_3_img"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/rm_3_selected_v1"
        android:scaleType="centerCrop"/>


    <ImageView
        android:id="@+id/hitboxes"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/hitbox_img_v3"
        android:scaleType="centerCrop"
        android:visibility="invisible"
        />

</FrameLayout>
